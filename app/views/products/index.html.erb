<h4><%= link_to "Home", '/' %> | <%= link_to "Products", '/products' %></h4>
<h2>Products</h2>
<% if @products.empty? %>
  <p>There are no products available.</p>
<% else %>
  <% @products.each do |product| %>
    <h4><%= product.name %> - SKU:<%= product.sku %> - Price:$<%= product.current_price %> <h4>
  <% end %>
<% end %>
